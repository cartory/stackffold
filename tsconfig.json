{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "es2015",
		"module": "commonjs",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"assets": [
				"assets/*"
			],
			"lib": [
				"lib/utils/*",
				"lib/models/*",
				"lib/scripts/*"
			]
		}
	}
}